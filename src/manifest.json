{
  "package": "com.typescript.quickapp",
  "name": "TS写快应用示例",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1060",
  "icon": "/assets/images/logo.png",
  "deviceTypeList": [
    "phone"
  ],
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "pages/Main",
    "pages": {
      "pages/Main": {
        "component": "index"
      },
      "pages/About": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": false,
    "menuBarData": {
      "menuBar" : true,
      "menuBarStyle":"dark",
      "shareTitle":"TS写快应用示例",
      "shareDescription":"如何基于 Typescript 开发快应用，项目示例模版"
    },
    "pages": {
      "Main": {
        "titleBarText": "示例主页",
        "menu": false
      },
      "MainDetail": {
        "titleBarText": "示例详情页"
      },
      "About": {
        "menu": false
      }
    }
  }
}